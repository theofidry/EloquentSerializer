{
    "name": "theofidry/eloquent-serializer",
    "description": "Symfony Serializer bridge for the Eloquent ORM.",
    "license": "MIT",
    "authors": [
        {
            "name": "Th√©o FIDRY",
            "email": "theo.fidry@gmail.com"
        }
    ],
    "type": "laravel-provider",
    "require": {
        "php": "^5.6.0||^7.0",
        "symfony/property-access": "^2.8||^3.0",
        "symfony/property-info": "^2.8||^3.0",
        "symfony/serializer": "^3.1"
    },
    "require-dev": {
        "doctrine/dbal": "^2.5",
        "laravel/framework": "^5.0",
        "symfony/framework-bundle": "^2.8||^3.0",
        "symfony/yaml": "^3.1",
        "wouterj/eloquent-bundle": "^0.2.2"
    },
    "autoload": {
        "psr-4": { "Fidry\\EloquentSerializer\\": "src" }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Fixtures/LaravelApp/database/migrations"
        ],
        "psr-4": { "Fidry\\EloquentSerializer\\": "tests" }
    },
    "config": {
        "sort-packages": true
    }
}
